<optimization>

    <genetic-configuration>
		<param name="ELITISM_K" type="Integer" value="2"></param>
		<param name="POP_SIZE" type="Integer" value="40"></param>
		<param name="MAX_ITER" type="Integer" value="10"></param>
		<param name="MUTATION_RATE" type="Double" value="0.02"></param>
		<param name="CROSSOVER_RATE" type="Double" value="0.7"></param>
		<param name="TOURNAMENT_SIZE" type="Integer" value="4"></param>
		<param name="INDIVIDUALS_SIZE" type="Integer" value="2"></param>
	</genetic-configuration>
	
	<wfa-configuration>
		<param name="OPTIMIZATION_WINDOW_PERCENTAGE" type="Integer" value="90"></param>
		<param name="WFA_WINDOW_PERCENTAGE" type="Integer" value="11"></param>
	</wfa-configuration>
	
	<indicators>
		<indicator network="TradeNetwork-6" name="_ADX" port="WINDOW">
			<interval start="14"  end="14" type="Double"></interval>
		</indicator>
		<indicator network="TradeNetwork-6" name="_SAR_SIGNAL_THRESHOLD" port="IN">
			<interval start="2"  end="8" type="Double"></interval>
		</indicator>	
		<indicator network="TradeNetwork-6" name="_AVGDX_THRESHOLD" port="IN">
			<interval start="25"  end="35" type="Double"></interval>
		</indicator>
		
	 
		<indicator network="TradeNetwork-6"  name="_ParabolicSAR" port="STEP">
			<interval start="0.02"  end="0.02" type="Double"></interval>
		</indicator>	
		<indicator network="TradeNetwork-6"  name="_ParabolicSAR" port="MAXSTEP">
			<interval start="0.2"  end="0.2" type="Double"></interval>
		</indicator>				
		<indicator network="TradeNetwork-6"  name="_ATR" port="WINDOW">
			<interval start="1"  end="1" type="Double"></interval>
		</indicator>
	</indicators>
		 	
		 

</optimization>
