<optimization>

	<configuration>
		<param name="PERIOD_TYPE" type="String" value="Day"></param>
		<param name="PERIOD_STEP_SIZE" type="Integer" value="1"></param>
		<param name="PERIOD_START" type="Date" value="20010110-204500"></param>
		<param name="PERIOD_END" type="Date" value="20011010-204500"></param>
		<param name="INPUT_MARKET_DATA_FILE_NAME" type="String" value="EURUSD.txt"></param>
		<param name="ACCOUNT_BALANCE" type="Double" value="100000d"></param>
		<param name="INPUT_TEST_TRADE_DATA_FILE_NAME" type="String" value="TestTradeSet-1"></param>
	</configuration>

    <genetic-configuration>
		<param name="ELITISM_K" type="Integer" value="1"></param>
		<param name="POP_SIZE" type="Integer" value="4"></param>
		<param name="MAX_ITER" type="Integer" value="1000"></param>
		<param name="MUTATION_RATE" type="Double" value="0.02"></param>
		<param name="CROSSOVER_RATE" type="Double" value="0.7"></param>
	</genetic-configuration>
	
	<indicators>
		<indicator network="TradeNetwork-1" name="_SimpleMovingAverage_SHORT" port="WINDOW">
			<interval start="1"  end="15" type="Double"></interval>
		</indicator>
		<indicator network="TradeNetwork-1" name="_SimpleMovingAverage_LONG" port="WINDOW">
			<interval start="16"  end="50" type="Double"></interval>
		</indicator>
				
	</indicators>
	

</optimization>
